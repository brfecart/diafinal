modules = ["nodejs-20", "web"]
[agent]
expertMode = true
integrations = ["javascript_gemini:1.0.0", "javascript_openai:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MandalaMind Dev Server"

[[workflows.workflow]]
name = "MandalaMind Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd SCCP1910/fecart-15-09/MandalaMind && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36193
externalPort = 3000

[deployment]
deploymentTarget = "vm"
run = ["bash", "-c", "cd SCCP1910/fecart-15-09/MandalaMind && npm run start"]
build = ["bash", "-c", "cd SCCP1910/fecart-15-09/MandalaMind && npm run build"]
